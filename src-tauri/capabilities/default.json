{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "stronghold:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/incident-workbench-api",
          "sidecar": true,
          "args": true
        }
      ]
    },
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/incident-workbench-api",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
